/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

IDR_MAINFRAME MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open\tCtrl+O",               ID_FILE_OPEN
        MENUITEM "E&xit",                       ID_APP_EXIT
    END
    POPUP "Player"
    BEGIN
        MENUITEM "&Play",                       ID_PLAY
        MENUITEM "&Stop",                       ID_STOP
        MENUITEM "P&ause",                      ID_PAUSE
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Menu bar",                   ID_VIEW_MENUBAR
        POPUP "&Toolbars"
        BEGIN
            MENUITEM "&Standard",                   ID_VIEW_TOOLBAR
            MENUITEM "&UI Style",                   ID_VIEW_UI_LOOK_BAR
            MENUITEM "&Player",                     IDR_TOOLBAR_PLAYER
            MENUITEM "&Zoom",                       IDR_TOOLBAR_ZOOM
            MENUITEM "S&eek",                       IDR_TOOLBAR_SEEK
        END
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        POPUP "&UI theme"
        BEGIN
            MENUITEM "<<< Prof-UIS Themes List >>>", 29999
        END
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About AviFrames...",         ID_APP_ABOUT
    END
END


/////////////////////////////////////////////////////////////////////////////
//
// Accelerator
//

IDR_MAINFRAME ACCELERATORS MOVEABLE PURE 
